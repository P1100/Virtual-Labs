{
  "firstName": "virtuallabs-frontend",
  "version": "1.0.2-SNAPSHOT",
  "scripts": {
    "start": "npm run serve-prod",
    "startAll-windows": "start npm run jsonserver & start npm run serve",
    "startAll-linux": "npm run jsonserver & npm run serve",
    "ng": "ng",
    "serve": "ng serve --base-href / --deploy-url /",
    "serve-prod": "ng serve --configuration production true --base-href / --deploy-url /",
    "serve-prod-ssl": "ng serve --configuration production true --ssl true --base-href / --deploy-url / --ssl-cert 'src/assets/server.crt' --ssl-key 'server.key'",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/localize": "^12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^4.6.0",
    "moment": "^2.29.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@types/jasmine": "^3.7.7",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.17.4",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "^4.3.5"
  }
}
