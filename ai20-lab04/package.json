{
  "firstName": "virtuallabs-frontend",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "start": "npm run serve-prod",
    "startAll-windows": "start npm run jsonserver & start npm run serve",
    "startAll-linux": "npm run jsonserver & npm run serve",
    "ng": "ng",
    "serve": "ng serve --prod false --base-href / --deploy-url /",
    "serve-prod": "ng serve --prod true --base-href / --deploy-url /",
    "serve-prod-ssl": "ng serve --prod true --ssl true --base-href / --deploy-url / --ssl-cert 'src/assets/server.crt' --ssl-key 'server.key'",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/localize": "~10.1.2",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.0",
    "moment": "^2.28.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "^10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "^4.0.3"
  }
}
